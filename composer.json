{
  "name": "erdmannfreunde/contao-gitlab-trigger",
  "description": "Trigger GitLab pipelines within Contao.",
  "type": "contao-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Richard Henkenjohann",
      "email": "richardhenkenjohann@googlemail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/erdmannfreunde/contao-gitlab-trigger/issues",
    "source": "https://github.com/erdmannfreunde/contao-gitlab-trigger"
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "contao/core-bundle": "^4.4",
    "defuse/php-encryption": "^2.2",
    "guzzlehttp/guzzle": "^6.3",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4",
    "symfony/config": "^3.3 || ^4.0",
    "symfony/http-kernel": "^3.3 || ^4.0",
    "symfony/routing": "^3.3 || ^4.0",
    "symfony/http-foundation": "^3.3 || ^4.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.7",
    "eblick/contao-trigger": "^1.0.3",
    "friendsofphp/php-cs-fixer": "^2.12",
    "m4tthumphrey/php-gitlab-api": "^9.13",
    "zeichen32/gitlabapibundle": "^3.1"
  },
  "suggest": {
    "eblick/contao-trigger": "Run GitLab pipelines on certain conditions by using the Contao trigger framework",
    "zeichen32/gitlabapibundle": "To be able to fetch the current status of the pipeline (recommended)"
  },
  "autoload": {
    "psr-4": {
      "ErdmannFreunde\\ContaoGitlabTriggerBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "ErdmannFreunde\\ContaoGitlabTriggerBundle\\ContaoManager\\Plugin"
  }
}
